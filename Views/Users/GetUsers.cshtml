@model WebMia.Models.Rol

<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
<link href="~/css/site.css" rel="stylesheet" />
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
<script src="~/js/site.js"></script>
<link href="~/css/DisplayDataStyle.css" rel="stylesheet" />

@{
    ViewData["Title"] = "Usuarios Registrados";
    Layout = "_UserGestionLayout";

}

<div class="table-responsive">
    <div style="padding-left:5em;">
        <table>
            <tr id="DisplayData" style="height:2.5em">
                <td style="padding-left:2em;">NOMBRE</td>
                <td style="padding-left:23em">ROL</td>
                <td style="width:100%; "></td>
            </tr>
        </table>

        <table id="tableData" class="table table-hover" background-color:lightgrey">

            @if (ViewBag.ActiveUsers != null)
            {
                @foreach (KeyValuePair<string, string> item in ViewBag.ActiveUsers)
                {

                    <tr>
                        <td contenteditable="false" style="padding-left:2em; width:6em; min-width: 6em; max-width:6em;">@item.Key</td>
                        <td contenteditable="false" style="padding-left:2em; width:10em; min-width: 10em; max-width:10em;">@item.Value</td>
                    </tr>
                }

            }

        </table>

    </div>
</div>